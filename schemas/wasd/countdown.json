{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "value": {
      "type": "number"
    },
    "state": {
      "enum": [
        "paused",
        "running",
        "ended"
      ]
    },
    "msg": {
      "type": "string"
    }
  },
  "required": [
    "value",
    "state",
    "msg"
  ],
  "default": {
    "value": 0,
    "state": "ended",
    "msg": "Back Soon"
  }
}