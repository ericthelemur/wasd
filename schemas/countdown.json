{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "display": {
      "type": "string"
    },
    "state": {
      "enum": [
        "paused",
        "running",
        "ended"
      ]
    },
    "msg": {
      "type": "string"
    }
  },
  "required": [
    "display",
    "state",
    "msg"
  ],
  "default": {
    "display": "00:00",
    "state": "ended",
    "msg": "Back Soon"
  }
}