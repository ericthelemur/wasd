{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "enabled": {
            "type": "boolean"
        },
        "ip": {
            "type": "string"
        },
        "localPort": {
            "type": "integer"
        },
        "xr18": {
            "type": "boolean"
        },
        "suppress": {
            "type": "boolean"
        }
    },
    "required": [
        "enabled",
        "ip",
        "xr18"
    ],
    "default": {
        "enabled": true,
        "ip": "192.168.1.99",
        "localPort": 10024,
        "xr18": true
    }
}