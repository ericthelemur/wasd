{
  "name": "wasd",
  "version": "1.0.0",
  "description": "",
  "homepage": "",
  "author": {
    "name": "ericthelemur",
    "email": "owenconnors1@gmail.com",
    "url": ""
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^2.0.0",
    "bundleDependencies": {
      "nodecg-ticker-control": "*"
    },
    "graphics": [
      {
        "file": "bar.graphic.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": false
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "node scripts/build.mjs --all",
    "build:extension": "node scripts/build.mjs --extension",
    "build:graphics": "node scripts/build.mjs --graphics",
    "build:dashboard": "node scripts/build.mjs --dashboard",
    "build:schema": "trash src/types/schemas && nodecg schema-types",
    "watch": "node scripts/build.mjs --all --watch",
    "watch:browser": "node scripts/build.mjs --dashboard --graphics --watch",
    "watch:schemas": "node scripts/build.mjs --schemas --watch",
    "dev": "concurrently --kill-others \"npm run watch:schemas\" \"npm run watch:browser\" \"nodemon\""
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-bootstrap": "^2.9.1",
    "react-bootstrap-icons": "^1.10.3",
    "react-contenteditable": "^3.3.7",
    "react-css-transition-replace": "^5.0.1",
    "react-dom": "^18.0.0",
    "react-fitty": "^1.0.1",
    "react-textfit": "github:ericthelemur/react-textfit",
    "react-transition-group": "^4.4.5",
    "ts-node": "^10.9.1",
    "use-nodecg": "github:ericthelemur/use-nodecg"
  },
  "devDependencies": {
    "@nodecg/types": "^2.0.0",
    "@parcel/config-default": "*",
    "@parcel/core": "*",
    "@parcel/reporter-cli": "*",
    "@parcel/transformer-sass": "^2.10.2",
    "@parcel/validator-typescript": "*",
    "@types/node": "^18",
    "@types/obs-studio": "^2.17.2",
    "@types/react": "^18.2.45",
    "@types/react-beautiful-dnd": "^13.1.7",
    "@types/react-css-transition-replace": "^2.1.7",
    "@types/react-dom": "^18.2.18",
    "@types/react-textfit": "^1.1.4",
    "concurrently": "*",
    "glob": "^10.2.7",
    "nodemon": "*",
    "trash-cli": "*",
    "typescript": "^5.1.3"
  },
  "license": "MIT"
}